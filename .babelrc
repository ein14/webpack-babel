{
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false,
          "targets": {
            "chrome": "73",
            "ie": "11"
          },
          "useBuiltIns": "usage",
          "corejs": 2
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-transform-async-to-generator"
    ]
  }